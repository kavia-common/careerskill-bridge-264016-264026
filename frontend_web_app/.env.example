# Frontend environment example - SkillBridge LMS
# Map these in deployment; do not commit real .env with secrets

# Base URL for REST API (backend)
REACT_APP_API_BASE=https://vscode-internal-34534-beta.beta01.cloud.kavia.ai:3001

# Public backend URL (optional; used in links)
REACT_APP_BACKEND_URL=https://vscode-internal-34534-beta.beta01.cloud.kavia.ai:3001

# Public frontend URL (current dev server)
REACT_APP_FRONTEND_URL=https://vscode-internal-34534-beta.beta01.cloud.kavia.ai:3000

# WebSocket endpoint (must be ws or wss)
REACT_APP_WS_URL=wss://vscode-internal-34534-beta.beta01.cloud.kavia.ai:3001/ws/notifications

# Node-like env label
REACT_APP_NODE_ENV=development

# Misc toggles
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=1
REACT_APP_TRUST_PROXY=0
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=0
