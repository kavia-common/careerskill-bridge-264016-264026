# Frontend environment configuration
# Base URL for REST API (e.g., http://localhost:3001 or https://api.example.com)
REACT_APP_API_BASE=http://localhost:3001
# Alternative env name supported for base URL
REACT_APP_BACKEND_URL=

# WebSocket URL for notifications (e.g., ws://localhost:3001/ws/notifications or wss://api.example.com/ws/notifications)
REACT_APP_WS_URL=ws://localhost:3001/ws/notifications

# The frontend public URL (useful for redirects if backend needs it)
REACT_APP_FRONTEND_URL=http://localhost:3000

# JSON or comma-separated flags, e.g. {"betaRecommendations":true} or "betaRecommendations,newUI"
REACT_APP_FEATURE_FLAGS=

# Log level: error | warn | info | debug
REACT_APP_LOG_LEVEL=info
